{
  "dashboard": {
    "id": null,
    "title": "Jenkins Energy Monitoring",
    "tags": ["jenkins", "energy", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Power Consumption",
        "type": "stat",
        "targets": [
          {
            "expr": "system_power_consumption_watts",
            "legendFormat": "Total Power (W)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 150
                },
                {
                  "color": "red",
                  "value": 250
                }
              ]
            },
            "unit": "watt"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Energy Efficiency",
        "type": "stat",
        "targets": [
          {
            "expr": "energy_efficiency_percent",
            "legendFormat": "Efficiency (%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 30
                },
                {
                  "color": "green",
                  "value": 60
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Carbon Footprint",
        "type": "stat",
        "targets": [
          {
            "expr": "carbon_footprint_kg_co2",
            "legendFormat": "CO2 (kg)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.05
                },
                {
                  "color": "red",
                  "value": 0.1
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Active Jenkins Builds",
        "type": "stat",
        "targets": [
          {
            "expr": "jenkins_active_builds",
            "legendFormat": "Active Builds"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Power Consumption Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "system_power_consumption_watts",
            "legendFormat": "Total Power"
          },
          {
            "expr": "cpu_power_consumption_watts",
            "legendFormat": "CPU Power"
          },
          {
            "expr": "memory_power_consumption_watts",
            "legendFormat": "Memory Power"
          },
          {
            "expr": "jenkins_power_consumption_watts",
            "legendFormat": "Jenkins Power"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "watt"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Build Energy Consumption",
        "type": "timeseries",
        "targets": [
          {
            "expr": "jenkins_build_energy_consumption_wh",
            "legendFormat": "Build {{ $labels.job_name }}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "watt"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Energy Efficiency Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "energy_efficiency_percent",
            "legendFormat": "Efficiency %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Carbon Footprint Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "carbon_footprint_kg_co2",
            "legendFormat": "CO2 (kg)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "Build Duration vs Energy Consumption",
        "type": "scatter",
        "targets": [
          {
            "expr": "jenkins_build_duration_seconds",
            "legendFormat": "Duration (s)"
          },
          {
            "expr": "jenkins_build_energy_consumption_wh",
            "legendFormat": "Energy (Wh)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "System Resource Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "system_cpu_usage_percent",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "system_memory_usage_percent",
            "legendFormat": "Memory Usage %"
          },
          {
            "expr": "system_disk_usage_percent",
            "legendFormat": "Disk Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}





